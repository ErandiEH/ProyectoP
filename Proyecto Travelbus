#include <stdio.h>
#include <stdlib.h>
#include <string.h>

#define MAX_USUARIOS 100
#define MAX_NOMBRE 50
#define MAX_CORREO 100

typedef struct {
  int numero;
  int ocupado;
} Asiento;
typedef struct {
  char nombre [MAX NOMBRE];
  char correo [MAX_CORREO];
  char telefono[15];
} Usuario;
typdef struct {
  char nombre[50];
  int capacidad_maxima;
  Asiento asientos[60];
  char rutas [5][50];
  int num_rutas;
} TipoAutobus;

